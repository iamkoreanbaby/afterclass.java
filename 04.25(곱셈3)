package Class;

import java.util.Scanner;

public class Class03 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("몇단까지?");
		int 몇단 = sc.nextInt();
		System.out.println("몇까지?");
		int 곱할수 = sc.nextInt();
		
		for(int i=1; i<=곱할수; i++) {
			for(int a=2;a<=몇단;a++) {
				System.out.println(a + "*"+ i + "="+a*i);
			}
			
		}
	}

}
