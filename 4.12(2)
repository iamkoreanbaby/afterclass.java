package javaclass;

public class Class1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int x = 1;
	int y = 2;
	int z = 3;
	int a = 0;
	a = x;
	x =	y;
	y = z;
	z = a;
	System.out.println("x=" + x);
	System.out.println("y=" + y);
	System.out.println("z=" + z);
	
	}

}
