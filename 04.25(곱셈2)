package Class;

import java.util.Scanner;

public class After01{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int 몇단 = sc.nextInt();
		int 곱=1;
		
		while(곱<=9) {
			System.out.println(몇단+"*"+곱+"="+(몇단*곱));
			곱++;
		
		
		}
		
		
	}

}
